# Variables de entorno para Caocal
# Copia este archivo a .env y ajusta los valores según tu entorno

# Entorno de desarrollo
VITE_APP_ENV=development
VITE_APP_NAME=Caocal
VITE_APP_VERSION=1.0.0

# Configuración de API
VITE_API_BASE_URL=http://localhost:3008
VITE_API_TIMEOUT=10000

# Configuración de analytics (opcional)
VITE_ANALYTICS_ID=

# Configuración de features
VITE_ENABLE_AI_RECOMMENDATIONS=true
VITE_ENABLE_REAL_TIME_UPDATES=true
VITE_ENABLE_PUSH_NOTIFICATIONS=false

# Configuración de desarrollo
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_PERFORMANCE_MONITORING=true 